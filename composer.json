  "require": {
    "php": ">=7.2",
    "spiral/core": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.5",
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpstan/phpstan": "^0.12.83",
    "phpstan/phpstan-phpunit": "^0.12.18",
    "phpstan/extension-installer": "^1.1",
    "infection/infection": ">=0.15.3"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Bridge\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Bridge\\Core\\Test\\": "tests/"
    }
